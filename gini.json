{
    "id": "gini",
    "name": "Gini PHP Framework",
    "author": "iamfat@gmail.com",
    "version": "1.12.2",
    "build": {
        "copy": [
            "bin",
            "lib",
            "raw",
            "data",
            "composer.json",
            "vendor"
        ]
    },
    "composer": {
        "name": "iamfat/gini",
        "description": "Gini PHP Framework",
        "license": "MIT",
        "type": "library",
        "authors": [
            {
                "name": "Jia Huang",
                "email": "iamfat@gmail.com"
            }
        ],
        "bin": [
            "bin/gini"
        ],
        "require": {
            "sabre/dav": "@stable",
            "doctrine/inflector": "@stable",
            "doctrine/annotations": "@stable",
            "dragonmantank/cron-expression": "@stable"
        },
        "require-dev": {
            "henrikbjorn/lurker": "1.*"
        }
    },
    "dependencies": [],
    "moduleClass": "\\Gini\\Module\\gini"
}
